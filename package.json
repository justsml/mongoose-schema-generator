    "csv-parse": "4.14.1",
    "debug": "^4.1.1",
    "functional-promises": "2.0.2",
    "lodash.camelcase": "^4.3.0",
    "lodash.capitalize": "^4.2.1",
    "lodash.isdate": "4.0.1",
    "lodash.isnumber": "3.0.3",
    "lodash.snakecase": "4.1.1",
    "node-sass": "^4.13.1",
    "react": "16.8.0",
    "react-apexcharts": "1.3.7",
    "react-dom": "16.8.0",
